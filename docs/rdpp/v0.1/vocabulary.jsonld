        "@type": "rdfs:Class",
        "rdfs:subClassOf": ["cred:CredentialSubject"],
        "rdfs:label": "Transport",
        "rdfs:comment": {
            "@value": "<div>Models a transport credential subject capturing responsible service provider, time-bound movement data, and environmental impact metrics. Transport aligns with the transactional model’s business steps (e.g., shipping) and event times, and service providers are represented as stakeholder roles within the DPP ecosystem.</div>",
            "@type": "http://www.w3.org/1999/02/22-rdf-syntax-ns#HTML"
        },
        "vs:term_status": "stable",
        "mentioned": [{
            "@id": "https://railway-dpp-vocabulary.spherity.dev/railway-dpp-context/v0.1",
            "@type": "jsonld:Context"
