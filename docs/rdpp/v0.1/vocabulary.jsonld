        "@type": "rdfs:Class",
        "rdfs:subClassOf": ["cred:CredentialSubject"],
        "rdfs:label": "Declaration of Product Conformity",
        "rdfs:comment": {
            "@value": "<div>Declaration of Product Conformity modeled as a credential subject. In the DPP semantic model, such declarations are treated as information products with metadata (issuer, issue date) and references to applicable standards.</div>",
