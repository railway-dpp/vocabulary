    "rdfs_classes": [{
        "@id": "rddp:ProductGroup",
        "@type": "rdfs:Class",
        "rdfs:subClassOf": ["cred:CredentialSubject"],
        "rdfs:label": "Product Group",
        "rdfs:comment": {
            "@value": "<div>Groups rail products that share essential technical characteristics (such as rail profile and steel grade) so they can be managed and assessed consistently within a Digital Product Passport. In the DPP System Standard semantic model, such characteristics are represented as product properties within a Product Property Collection, while related certificates and documents are modeled as information products. Steel grade-related acceptance and mechanical conformity are validated through tensile strength and elongation testing procedures defined in EN 13674-1.</div>",
            "@type": "http://www.w3.org/1999/02/22-rdf-syntax-ns#HTML"
        },
        "vs:term_status": "stable",
        "mentioned": [{
            "@id": "https://railway-dpp-vocabulary.spherity.dev/railway-dpp-context/v0.1",
            "@type": "jsonld:Context"
        }]
