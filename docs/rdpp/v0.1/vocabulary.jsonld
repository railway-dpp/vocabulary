        "rdfs:subClassOf": ["cred:CredentialSubject"],
        "rdfs:label": "EPD Production",
        "rdfs:comment": {
            "@value": "<div>Environmental Product Declaration describing environmental impacts associated with the production stage. Depending on granularity, EPDs may be modeled as information products and/or quantitative product properties.</div>",
