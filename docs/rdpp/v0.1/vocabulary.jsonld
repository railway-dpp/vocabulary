        "rdfs:subClassOf": ["cred:CredentialSubject"],
        "rdfs:label": "Digital Product Passport",
        "rdfs:comment": {
            "@value": "<div>The Digital Product Passport acts as the digital container for product metadata, product properties, and lifecycle-related information. Core metadata includes identifiers, product name, granularity, and facility references, while properties such as length and weight are modeled as data elements with controlled units.</div>",
            "@type": "http://www.w3.org/1999/02/22-rdf-syntax-ns#HTML"
        },
        "vs:term_status": "stable",
